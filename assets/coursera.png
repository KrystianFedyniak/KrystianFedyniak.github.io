<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="courseraGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0056D2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0077CC;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Фон сертификата -->
  <rect width="300" height="200" fill="url(#courseraGrad)" rx="15"/>
  
  <!-- Логотип Coursera -->
  <g transform="translate(150, 80)">
    <circle cx="0" cy="0" r="25" fill="white"/>
    <text x="0" y="8" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#0056D2">C</text>
  </g>
  
  <!-- Текст сертификата -->
  <text x="150" y="130" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="white">UX/UI Design</text>
  <text x="150" y="150" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="white">Certificate of Completion</text>
  <text x="150" y="170" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="white">2020</text>
  
  <!-- Декоративные элементы -->
  <circle cx="50" cy="50" r="3" fill="white" opacity="0.3"/>
  <circle cx="250" cy="50" r="3" fill="white" opacity="0.3"/>
  <circle cx="50" cy="150" r="3" fill="white" opacity="0.3"/>
  <circle cx="250" cy="150" r="3" fill="white" opacity="0.3"/>
</svg> 